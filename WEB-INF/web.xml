        <servlet-name>PreviousPage</servlet-name>
        <servlet-class>com.kd.kineticSurvey.servlets.PreviousPage</servlet-class>
    </servlet>
    <servlet>
        <description>Default Authentication support</description>
        <servlet-name>KSAuthenticationServlet</servlet-name>
        <servlet-class>com.kd.kineticSurvey.servlets.KSAuthenticationServlet</servlet-class>
