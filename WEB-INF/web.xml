    </servlet>
    <servlet>
        <description>Service Data request into Remedy</description>
        <servlet-name>SimpleDataRequest</servlet-name>
        <servlet-class>com.kd.kineticSurvey.servlets.SimpleDataRequest</servlet-class>
    </servlet>
    <servlet>
